{
  "name": "poof-v2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/poofcash/poof-v2.git",
  "author": "Poof.cash team",
  "license": "MIT",
  "files": [
    "index.js",
    "src/*",
    "contracts/*"
  ],
  "scripts": {
    "compile": "truffle compile",
    "test": "truffle test",
    "test:nonpoof": " truffle test test/feeBase.test.js test/wrappedCToken.test.js test/wrappedMToken.test.js test/wrappedATokenVal.test.js",
    "test:abridged": "truffle test --grep=\"should work\" test/poof.base.test.js test/poof.aux.test.js test/poofval.base.test.js test/poofval.aux.test.js && yarn test:nonpoof",
    "prettier:check": "prettier --check . --config .prettierrc",
    "prettier:fix": "prettier --write . --config .prettierrc",
    "circuit:setup": "scripts/buildCircuit.sh",
    "circuit:contribute": "scripts/contributeCircuit.sh",
    "circuit:finalize": "scripts/finalizeCircuit.sh",
    "circuit:quickSetup": "yarn circuit:setup && yarn circuit:finalize 0000",
    "slither": "slither --exclude-dependencies --exclude-informational --exclude-low --exclude-medium --exclude=arbitrary-send --filter-paths=WrappedATokenVal.sol ."
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "bn-chai": "^1.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "circom": "0.5.45",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.6.2",
    "mocha": "^9.1.1",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "rlp": "^2.2.6",
    "solhint-plugin-prettier": "^0.0.4",
    "solidity-coverage": "^0.7.7",
    "truffle": "^5.1.29",
    "truffle-flattener": "^1.4.4",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.5.15"
  },
  "dependencies": {
    "@truffle/hdwallet-provider": "^1.5.0",
    "big-integer": "^1.6.48",
    "circomlib": "0.5.3",
    "decimal.js": "^10.2.0",
    "dotenv": "^10.0.0",
    "eth-sig-util": "^2.5.3",
    "ffjavascript": "^0.2.38",
    "fixed-merkle-tree": "^0.3.4",
    "snarkjs": "npm:@poofcash/snarkjs@0.0.1",
    "web3": "^1.5.2",
    "websnark": "^0.0.5"
  }
}
